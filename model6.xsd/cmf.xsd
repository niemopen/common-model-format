<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
  targetNamespace="https://docs.oasis-open.org/niemopen/ns/specification/cmf/0.8/"
  xmlns:appinfo="https://docs.oasis-open.org/niemopen/ns/model/appinfo/6.0/"
  xmlns:cmf="https://docs.oasis-open.org/niemopen/ns/specification/cmf/0.8/"
  xmlns:ct="https://docs.oasis-open.org/niemopen/ns/specification/conformanceTargets/3.0/"
  xmlns:nc="https://docs.oasis-open.org/niemopen/ns/model/niem-core/6.0/"
  xmlns:structures="https://docs.oasis-open.org/niemopen/ns/model/structures/6.0/"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xs-proxy="https://docs.oasis-open.org/niemopen/ns/model/proxy/niem-xs/6.0/"
  ct:conformanceTargets="https://docs.oasis-open.org/niemopen/ns/specification/naming-and-design-rules/6.0/#ExtensionSchemaDocument"
  version="1"
  xml:lang="en-US">
  <xs:annotation>
    <xs:documentation>A data model for the NIEM Common Model Format (CMF).</xs:documentation>
  </xs:annotation>
  <xs:import namespace="https://docs.oasis-open.org/niemopen/ns/model/niem-core/6.0/" schemaLocation="niem/niem-core.xsd"/>
  <xs:import namespace="https://docs.oasis-open.org/niemopen/ns/model/proxy/niem-xs/6.0/" schemaLocation="niem/adapters/niem-xs.xsd"/>
  <xs:import namespace="https://docs.oasis-open.org/niemopen/ns/model/structures/6.0/" schemaLocation="niem/utility/structures.xsd"/>
  <xs:complexType name="AnyValueFacetType">
    <xs:annotation>
      <xs:documentation>A data type for a facet that can have any value type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:AnyValueAbstract"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:AnyValueFacetAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AugmentRecordType">
    <xs:annotation>
      <xs:documentation>A data type for a class that is augmented with a property by a namespace.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:Class" minOccurs="0"/>
          <xs:element ref="cmf:Property"/>
          <xs:element ref="cmf:AugmentationIndex"/>
          <xs:element ref="cmf:MinOccursQuantity"/>
          <xs:element ref="cmf:MaxOccursQuantity"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ClassType">
    <xs:annotation>
      <xs:documentation>A data type for a class.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cmf:ComponentType">
        <xs:sequence>
          <xs:element ref="cmf:AugmentableIndicator" minOccurs="0"/>
          <xs:element ref="cmf:ExternalAdapterTypeIndicator" minOccurs="0"/>
          <xs:element ref="cmf:ExtensionOfClass" minOccurs="0"/>
          <xs:element ref="cmf:HasProperty" minOccurs="0" maxOccurs="unbounded" appinfo:orderedPropertyIndicator="true"/>
          <xs:element ref="cmf:ClassAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CodeListBindingType">
    <xs:annotation>
      <xs:documentation>A data type for connecting simple content defined by an XML Schema component to a a column of a code list.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:CodeListURI"/>
          <xs:element ref="cmf:CodeListColumnName" minOccurs="0"/>
          <xs:element ref="cmf:CodeListConstrainingIndicator" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ComponentType" abstract="true">
    <xs:annotation>
      <xs:documentation>A data type for common properties of a data component in NIEM.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:Name"/>
          <xs:element ref="cmf:Namespace"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:AbstractIndicator" minOccurs="0"/>
          <xs:element ref="cmf:DeprecatedIndicator" minOccurs="0"/>
          <xs:element ref="cmf:ComponentAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DatatypeType">
    <xs:annotation>
      <xs:documentation>A data type for a data type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cmf:ComponentType">
        <xs:sequence>
          <xs:element ref="cmf:RestrictionOf" minOccurs="0"/>
          <xs:element ref="cmf:UnionOf" minOccurs="0"/>
          <xs:element ref="cmf:ListOf" minOccurs="0"/>
          <xs:element ref="cmf:CodeListBinding" minOccurs="0"/>
          <xs:element ref="cmf:DatatypeAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HasPropertyType">
    <xs:annotation>
      <xs:documentation>A data type for an occurrence of a property as content of a class.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:Property"/>
          <xs:element ref="cmf:MinOccursQuantity"/>
          <xs:element ref="cmf:MaxOccursQuantity"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:OrderedPropertyIndicator" minOccurs="0"/>
          <xs:element ref="cmf:AugmentationNamespace" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:HasPropertyAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="MaxOccursSimpleType">
    <xs:annotation>
      <xs:documentation>A data type for the maximum number of times a property may occur within a class.</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="xs:nonNegativeInteger cmf:MaxOccursUnboundedCodeSimpleType"/>
  </xs:simpleType>
  <xs:complexType name="LocalTermType">
    <xs:annotation>
      <xs:documentation>A data type for the meaning of a term that may appear within the name of a model component.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:TermName"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:TermLiteralText" minOccurs="0"/>
          <xs:element ref="cmf:SourceURIList" minOccurs="0"/>
          <xs:element ref="cmf:SourceCitationText" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="MaxOccursType">
    <xs:annotation>
      <xs:documentation>A data type for the maximum number of times a property may occur within a class.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="cmf:MaxOccursSimpleType">
        <xs:anyAttribute processContents="lax"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:simpleType name="MaxOccursUnboundedCodeSimpleType">
    <xs:annotation>
      <xs:documentation>A data type for identifying that there is no maximum number of times that a property may occur within a class.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="unbounded">
        <xs:annotation>
          <xs:documentation>There is no maximum number of times that a property may occur.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="MaxOccursUnboundedCodeType">
    <xs:annotation>
      <xs:documentation>A data type for identifying that there is no maximum number of times that a property may occur within a class.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="cmf:MaxOccursUnboundedCodeSimpleType">
        <xs:anyAttribute processContents="lax"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="ModelType">
    <xs:annotation>
      <xs:documentation>A data type for a CMF data model.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:AugmentRecord" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:ModelItemAbstract" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:SchemaDocument" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:ModelAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="NCNameSimpleType">
    <xs:restriction base="xs:Name">
      <xs:pattern value="[\i-[:]][\c-[:]]*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="NCNameType">
    <xs:simpleContent>
      <xs:extension base="cmf:NCNameSimpleType">
        <xs:anyAttribute processContents="lax"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:simpleType name="NamespaceKindCodeSimpleType">
    <xs:restriction base="xs:token">
      <xs:enumeration value="EXTENSION">
        <xs:annotation>
          <xs:documentation>An extension namespace.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DOMAIN">
        <xs:annotation>
          <xs:documentation>A namespace for a NIEM domain.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CORE">
        <xs:annotation>
          <xs:documentation>The namespace for the NIEM core.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OTHERNIEM">
        <xs:annotation>
          <xs:documentation>A namespace in the publication or release domain, but not core, domain, or builtin.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BUILTIN">
        <xs:annotation>
          <xs:documentation>A builtin namespace (appinfo, code-lists, conformance, proxy, structures).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="XSD">
        <xs:annotation>
          <xs:documentation>http://www.w3.org/2001/XMLSchema</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="XML">
        <xs:annotation>
          <xs:documentation>http://www.w3.org/XML/1998/namespace</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EXTERNAL">
        <xs:annotation>
          <xs:documentation>A non-conforming namespace.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="UNKNOWN">
        <xs:annotation>
          <xs:documentation>A namespace of an unknown kind.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="NamespaceKindCodeType">
    <xs:simpleContent>
      <xs:extension base="cmf:NamespaceKindCodeSimpleType">
        <xs:anyAttribute processContents="lax"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="NamespaceType">
    <xs:annotation>
      <xs:documentation>A data type for a namespace</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:NamespaceURI"/>
          <xs:element ref="cmf:NamespacePrefixText"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:NamespaceKindCode" minOccurs="0"/>
          <xs:element ref="cmf:AugmentRecord" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:LocalTerm" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:NamespaceAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="NonNegativeValueFacetType">
    <xs:annotation>
      <xs:documentation>A data type for a facet that carries a value that is either zero or a positive integer.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:NonNegativeValue"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:NonNegativeValueFacetAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PatternFacetType">
    <xs:annotation>
      <xs:documentation>A data type for restricting the content of a datatype to values that match a specific pattern, provided as a regular expression.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:StringValue"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:PatternFacetAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PositiveValueFacetType">
    <xs:annotation>
      <xs:documentation>A data type for a facet that carries a value that is a positive integer.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:PositiveValue"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:PositiveValueFacetAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PropertyType">
    <xs:annotation>
      <xs:documentation>A data type for an object property</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cmf:ComponentType">
        <xs:sequence>
          <xs:element ref="cmf:SubPropertyOf" minOccurs="0"/>
          <xs:element ref="cmf:Class" minOccurs="0"/>
          <xs:element ref="cmf:Datatype" minOccurs="0"/>
          <xs:element ref="cmf:AttributeIndicator" minOccurs="0"/>
          <xs:element ref="cmf:RefAttributeIndicator" minOccurs="0"/>
          <xs:element ref="cmf:ReferenceableIndicator" minOccurs="0"/>
          <xs:element ref="cmf:RelationshipPropertyIndicator" minOccurs="0"/>
          <xs:element ref="cmf:PropertyAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="RestrictionOfType">
    <xs:annotation>
      <xs:documentation>A data type for a restriction of a data type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:Datatype"/>
          <xs:element ref="cmf:FacetAbstract" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="cmf:RestrictionOfAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SchemaDocumentType">
    <xs:annotation>
      <xs:documentation>A data type for an XML schema document.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:NamespacePrefixText" minOccurs="0"/>
          <xs:element ref="cmf:NamespaceURI"/>
          <xs:element ref="cmf:ConformanceTargetURIList" minOccurs="0"/>
          <xs:element ref="cmf:DocumentFilePathText" minOccurs="0"/>
          <xs:element ref="cmf:NIEMVersionText" minOccurs="0"/>
          <xs:element ref="cmf:SchemaVersionText" minOccurs="0"/>
          <xs:element ref="cmf:SchemaLanguageName" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="URIListSimpleType">
    <xs:annotation>
      <xs:documentation>A data type for a white space-delimited list of URIs.</xs:documentation>
    </xs:annotation>
    <xs:list itemType="xs:anyURI"/>
  </xs:simpleType>
  <xs:complexType name="URIListType">
    <xs:annotation>
      <xs:documentation>A data type for a white space-delimited list of URIs.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="cmf:URIListSimpleType">
        <xs:anyAttribute processContents="lax"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="UnionOfType">
    <xs:annotation>
      <xs:documentation>A data type for a union of data types.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:Datatype" maxOccurs="unbounded"/>
          <xs:element ref="cmf:UnionOfAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="WhiteSpaceFacetCodeSimpleType">
    <xs:annotation>
      <xs:documentation>A data type for a specification of white space for a data type.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="collapse">
        <xs:annotation>
          <xs:documentation>After the processing implied by replace, contiguous sequences of #x20's are collapsed to a single #x20, and leading and trailing #x20's are removed.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="preserve">
        <xs:annotation>
          <xs:documentation>No normalization is done, the value is not changed (this is the behavior required by [XML 1.0 (Second Edition)] for element content)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="replace">
        <xs:annotation>
          <xs:documentation>All occurrences of #x9 (tab), #xA (line feed) and #xD (carriage return) are replaced with #x20 (space)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="WhiteSpaceFacetCodeType">
    <xs:annotation>
      <xs:documentation>A data type for a specification of white space for a data type.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="cmf:WhiteSpaceFacetCodeSimpleType">
        <xs:anyAttribute processContents="lax"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="WhiteSpaceFacetType">
    <xs:annotation>
      <xs:documentation>A data type constraining how white space within data values is handled.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="cmf:WhiteSpaceValueCode"/>
          <xs:element ref="cmf:DefinitionText" minOccurs="0"/>
          <xs:element ref="cmf:WhiteSpaceFacetAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="WhiteSpaceValueCodeSimpleType">
    <xs:annotation>
      <xs:documentation>A data type for a specification of white space for a data type.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="collapse">
        <xs:annotation>
          <xs:documentation>After the processing implied by replace, contiguous sequences of #x20's are collapsed to a single #x20, and leading and trailing #x20's are removed.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="preserve">
        <xs:annotation>
          <xs:documentation>No normalization is done, the value is not changed (this is the behavior required by [XML 1.0 (Second Edition)] for element content)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="replace">
        <xs:annotation>
          <xs:documentation>All occurrences of #x9 (tab), #xA (line feed) and #xD (carriage return) are replaced with #x20 (space)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="WhiteSpaceValueCodeType">
    <xs:annotation>
      <xs:documentation>A data type for a specification of white space for a data type.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="cmf:WhiteSpaceValueCodeSimpleType">
        <xs:anyAttribute processContents="lax"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:element name="AbstractIndicator" type="xs-proxy:boolean">
    <xs:annotation>
      <xs:documentation>True if a component is a base for extension, and must be specialized to be used directly; false if a component may be used directly.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AnyValueAbstract" abstract="true">
    <xs:annotation>
      <xs:documentation>A data concept for a value of a facet.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AnyValueFacetAugmentationPoint" abstract="true">
    <xs:annotation>
      <xs:documentation>An augmentation point for AnyValueFacetType.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AttributeIndicator" type="xs-proxy:boolean">
    <xs:annotation>
      <xs:documentation>True for properties represented as attributes in XML.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AugmentRecord" type="cmf:AugmentRecordType">
    <xs:annotation>
      <xs:documentation>An augmentation of a class that is augmented with a property by a namespace.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AugmentableIndicator" type="xs-proxy:boolean">
    <xs:annotation>
      <xs:documentation>True if the class can be augmented with additional properties.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AugmentationIndex" type="xs-proxy:integer">
    <xs:annotation>
      <xs:documentation>An index of an augmentation property within an augmentation type. Negative (-1) for augmentation property not within an augmentation type.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="AugmentationNamespace" type="cmf:NamespaceType" nillable="true">
    <xs:annotation>
      <xs:documentation>A namespace responsible for augmenting a class with a property.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="Class" type="cmf:ClassType" substitutionGroup="cmf:ModelItemAbstract" nillable="true">
    <xs:annotation>
      <xs:documentation>A class.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ClassAugmentationPoint" abstract="true">
    <xs:annotation>
      <xs:documentation>An augmentation point for ClassType.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CodeListBinding" type="cmf:CodeListBindingType">
    <xs:annotation>
      <xs:documentation>An element for connecting simple content defined by an XML Schema component to a a column of a code list.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CodeListColumnName" type="xs-proxy:string">
    <xs:annotation>
      <xs:documentation>A local name for a code list column within a code list.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CodeListConstrainingIndicator" type="xs-proxy:boolean">
    <xs:annotation>
      <xs:documentation>True when a code list binding constrains the validity of a code list value, false otherwise.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="CodeListURI" type="xs-proxy:anyURI">
    <xs:annotation>
      <xs:documentation>A universal identifier for a code list.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ComponentAugmentationPoint" abstract="true">
    <xs:annotation>
      <xs:documentation>An augmentation point for ComponentType.</xs:documentation>
    </xs:annotation>
  </xs:element>
